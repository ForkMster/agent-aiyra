{
  "version": 2,
  "builds": [
    { "src": "src/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/env", "dest": "src/index.js" },
    { "src": "/test-reply", "dest": "src/index.js" },
    { "src": "/logs", "dest": "src/index.js" },
    { "src": "/poll", "dest": "src/index.js" },
    { "src": "/keepalive", "dest": "src/index.js" },
    { "src": "/(.*)", "dest": "src/index.js" }
  ],
  "crons": [
    { "path": "/keepalive", "schedule": "*/5 * * * *" },
    { "path": "/poll", "schedule": "*/1 * * * *" }
  ]
}